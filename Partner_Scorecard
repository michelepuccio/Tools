    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script>
        // Global variables
        let charts = {};
        let scores = {
            technical: 0,
            sales: 0,
            customer: 0,
            certifications: 0,
            business: 0,
            support: 0
        };
        
        function showTab(tabName) {
            // Hide all sections
            document.querySelectorAll('.scorecard-section').forEach(section => {
                section.classList.remove('active');
            });
            
            // Remove active class from all buttons
            document.querySelectorAll('.tab-button').forEach(button => {
                button.classList.remove('active');
            });
            
            // Show selected section
            document.getElementById(tabName).classList.add('active');
            
            // Add active class to clicked button
            event.target.classList.add('active');
            
            // Update dashboard if switching to it
            if (tabName === 'dashboard') {
                updateDashboard();
            }
        }
        
        function calculateScore(prefix, callback) {
            let totalScore = 0;
            let completedItems = 0;
            
            for (let i = 1; i <= 6; i++) {
                const value = parseInt(document.getElementById(`${prefix}-${i}`).value) || 0;
                if (value > 0) {
                    totalScore += value;
                    completedItems++;
                }
            }
            
            const maxScore = 30;
            const percentage = (totalScore / maxScore * 100);
            
            document.getElementById(`${prefix}-score`).textContent = `${totalScore}/${maxScore}`;
            document.getElementById(`${prefix}-percentage`).textContent = `${percentage.toFixed(1)}%`;
            
            let level = 'N/A';
            let indicator = 'Completa la valutazione per vedere il risultato';
            let indicatorClass = 'performance-indicator';
            
            if (completedItems === 6) {
                if (percentage >= 85) {
                    level = 'Eccellente';
                    indicator = 'üèÜ ECCELLENTE - Performance superiore';
                    indicatorClass = 'performance-indicator perf-excellent';
                } else if (percentage >= 70) {
                    level = 'Buono';
                    indicator = '‚úÖ BUONO - Performance soddisfacente';
                    indicatorClass = 'performance-indicator perf-good';
                } else if (percentage >= 55) {
                    level = 'Sufficiente';
                    indicator = '‚ö†Ô∏è SUFFICIENTE - Margini di miglioramento';
                    indicatorClass = 'performance-indicator perf-average';
                } else {
                    level = 'Insufficiente';
                    indicator = '‚ùå INSUFFICIENTE - Azione richiesta';
                    indicatorClass = 'performance-indicator perf-poor';
                }
            }
            
            document.getElementById(`${prefix}-level`).textContent = level;
            const indicatorElement = document.getElementById(`${prefix}-indicator`);
            indicatorElement.textContent = indicator;
            indicatorElement.className = indicatorClass;
            
            // Store score
            scores[prefix] = percentage;
            
            // Update chart
            if (callback) callback(totalScore, percentage);
            
            return { totalScore, percentage, level };
        }
        
        function calculateTechnical() {
            const result = calculateScore('technical', updateTechnicalChart);
            return result;
        }
        
        function calculateSales() {
            const result = calculateScore('sales', updateSalesChart);
            return result;
        }
        
        function calculateCustomer() {
            const result = calculateScore('customer', updateCustomerChart);
            return result;
        }
        
        function calculateCertifications() {
            const result = calculateScore('certifications', updateCertificationsChart);
            return result;
        }
        
        function calculateBusiness() {
            const result = calculateScore('business', updateBusinessChart);
            return result;
        }
        
        function calculateSupport() {
            const result = calculateScore('support', updateSupportChart);
            return result;
        }
        
        // Chart update functions
        function updateTechnicalChart(score, percentage) {
            const ctx = document.getElementById('technicalChart').getContext('2d');
            
            if (charts.technical) {
                charts.technical.destroy();
            }
            
            const labels = ['Conoscenza Prodotti', 'Implementazione', 'Troubleshooting', 'Aggiornamento', 'Documentazione', 'Integrazione'];
            const data = [];
            
            for (let i = 1; i <= 6; i++) {
                const value = parseInt(document.getElementById(`technical-${i}`).value) || 0;
                data.push(value);
            }
            
            charts.technical = new Chart(ctx, {
                type: 'radar',
                data: {
                    labels: labels,
                    datasets: [{
                        label: 'Competenze Tecniche',
                        data: data,
                        backgroundColor: 'rgba(68, 114, 196, 0.3)',
                        borderColor: '#4472C4',
                        borderWidth: 2,
                        pointBackgroundColor: '#4472C4'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        r: {
                            beginAtZero: true,
                            max: 5,
                            ticks: {
                                stepSize: 1
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            });
        }
        
        function updateSalesChart(score, percentage) {
            const ctx = document.getElementById('salesChart').getContext('2d');
            
            if (charts.sales) {
                charts.sales.destroy();
            }
            
            const data = [];
            for (let i = 1; i <= 6; i++) {
                const value = parseInt(document.getElementById(`sales-${i}`).value) || 0;
                data.push(value);
            }
            
            charts.sales = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: ['Obiettivi', 'Crescita', 'Nuovi Clienti', 'Portfolio', 'Marketing', 'Collaborazione'],
                    datasets: [{
                        label: 'Performance Vendite',
                        data: data,
                        backgroundColor: '#70AD47',
                        borderColor: '#5B8F3A',
                        borderWidth: 1
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            beginAtZero: true,
                            max: 5,
                            ticks: {
                                stepSize: 1
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            });
        }
        
        function updateCustomerChart(score, percentage) {
            const ctx = document.getElementById('customerChart').getContext('2d');
            
            if (charts.customer) {
                charts.customer.destroy();
            }
            
            charts.customer = new Chart(ctx, {
                type: 'doughnut',
                data: {
                    labels: ['Soddisfatti', 'Da Migliorare'],
                    datasets: [{
                        data: [percentage, 100 - percentage],
                        backgroundColor: ['#70AD47', '#E0E0E0'],
                        borderWidth: 0
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            position: 'bottom'
                        }
                    }
                }
            });
        }
        
        function updateCertificationsChart(score, percentage) {
            const ctx = document.getElementById('certificationsChart').getContext('2d');
            
            if (charts.certifications) {
                charts.certifications.destroy();
            }
            
            const data = [];
            for (let i = 1; i <= 6; i++) {
                const value = parseInt(document.getElementById(`cert-${i}`).value) || 0;
                data.push(value * 20); // Convert to percentage
            }
            
            charts.certifications = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: ['Tech Cert', 'Updates', 'Training', 'Partnership', 'Specialization', 'Knowledge'],
                    datasets: [{
                        label: 'Livello Certificazioni (%)',
                        data: data,
                        borderColor: '#FFA500',
                        backgroundColor: 'rgba(255, 165, 0, 0.3)',
                        borderWidth: 3,
                        fill: true,
                        tension: 0.4
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            beginAtZero: true,
                            max: 100,
                            ticks: {
                                callback: function(value) {
                                    return value + '%';
                                }
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            });
        }
        
        function updateBusinessChart(score, percentage) {
            const ctx = document.getElementById('businessChart').getContext('2d');
            
            if (charts.business) {
                charts.business.destroy();
            }
            
            const labels = ['Pianificazione', 'Innovazione', 'Partnership', 'Investimenti', 'Team', 'Sostenibilit√†'];
            const data = [];
            
            for (let i = 1; i <= 6; i++) {
                const value = parseInt(document.getElementById(`business-${i}`).value) || 0;
                data.push(value);
            }
            
            charts.business = new Chart(ctx, {
                type: 'radar',
                data: {
                    labels: labels,
                    datasets: [{
                        label: 'Business Development',
                        data: data,
                        backgroundColor: 'rgba(255, 99, 132, 0.3)',
                        borderColor: '#FF6384',
                        borderWidth: 2,
                        pointBackgroundColor: '#FF6384'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        r: {
                            beginAtZero: true,
                            max: 5,
                            ticks: {
                                stepSize: 1
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            });
        }
        
        function updateSupportChart(score, percentage) {
            const ctx = document.getElementById('supportChart').getContext('2d');
            
            if (charts.support) {
                charts.support.destroy();
            }
            
            const data = [];
            for (let i = 1; i <= 6; i++) {
                const value = parseInt(document.getElementById(`support-${i}`).value) || 0;
                data.push(value);
            }
            
            charts.support = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: ['Disponibilit√†', 'Tempi', 'Competenza', 'Escalation', 'Proattivit√†', 'Comunicazione'],
                    datasets: [{
                        label: 'Qualit√† Supporto',
                        data: data,
                        backgroundColor: '#9966CC',
                        borderColor: '#7A52A0',
                        borderWidth: 1
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            beginAtZero: true,
                            max: 5,
                            ticks: {
                                stepSize: 1
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            });
        }
        
        function updateDashboard() {
            // Update individual scores
            document.getElementById('dash-tech').textContent = scores.technical.toFixed(1) + '%';
            document.getElementById('dash-sales').textContent = scores.sales.toFixed(1) + '%';
            document.getElementById('dash-customer').textContent = scores.customer.toFixed(1) + '%';
            document.getElementById('dash-cert').textContent = scores.certifications.toFixed(1) + '%';
            document.getElementById('dash-business').textContent = scores.business.toFixed(1) + '%';
            document.getElementById('dash-support').textContent = scores.support.toFixed(1) + '%';
            
            // Calculate totals
            const totalScore = Object.values(scores).reduce((sum, score) => sum + score, 0);
            const averagePercentage = totalScore / 6;
            const maxPossible = 180; // 6 areas √ó 30 points each
            const actualTotal = (averagePercentage / 100) * maxPossible;
            
            document.getElementById('total-score').textContent = Math.round(actualTotal) + '/180';
            document.getElementById('total-percentage').textContent = averagePercentage.toFixed(1) + '%';
            
            // Determine partner tier
            let tier = 'N/A';
            let performanceClass = 'performance-indicator';
            let performanceText = 'Completa le valutazioni';
            
            if (averagePercentage > 0) {
                if (averagePercentage >= 85) {
                    tier = 'PLATINUM';
                    performanceText = 'üèÜ PLATINUM PARTNER - Excellence in tutte le aree';
                    performanceClass = 'performance-indicator perf-excellent';
                } else if (averagePercentage >= 70) {
                    tier = 'GOLD';
                    performanceText = 'ü•á GOLD PARTNER - Performance elevata';
                    performanceClass = 'performance-indicator perf-good';
                } else if (averagePercentage >= 55) {
                    tier = 'SILVER';
                    performanceText = 'ü•à SILVER PARTNER - Buona performance generale';
                    performanceClass = 'performance-indicator perf-average';
                } else {
                    tier = 'BRONZE';
                    performanceText = 'ü•â BRONZE PARTNER - Necessari miglioramenti';
                    performanceClass = 'performance-indicator perf-poor';
                }
            }
            
            document.getElementById('partner-tier').textContent = tier;
            
            const performanceElement = document.getElementById('overall-performance');
            performanceElement.textContent = performanceText;
            performanceElement.className = performanceClass;
            
            // Update improvement areas
            updateImprovementAreas();
            
            // Update dashboard chart
            updateDashboardChart();
        }
        
        function updateImprovementAreas() {
            const areas = [
                { name: 'Competenze Tecniche', score: scores.technical },
                { name: 'Performance Vendite', score: scores.sales },
                { name: 'Soddisfazione Clienti', score: scores.customer },
                { name: 'Certificazioni', score: scores.certifications },
                { name: 'Sviluppo Business', score: scores.business },
                { name: 'Qualit√† Supporto', score: scores.support }
            ];
            
            // Sort by score (lowest first)\n            areas.sort((a, b) => a.score - b.score);\n            \n            const improvementDiv = document.getElementById('improvement-areas');\n            \n            if (areas.every(area => area.score === 0)) {\n                improvementDiv.innerHTML = '<p style=\"text-align: center; color: #666; font-style: italic;\">Completa le valutazioni per vedere le raccomandazioni</p>';\n                return;\n            }\n            \n            let html = '';\n            areas.slice(0, 3).forEach((area, index) => {\n                const priority = index === 0 ? 'üî¥ Alta' : index === 1 ? 'üü° Media' : 'üü¢ Bassa';\n                html += `\n                    <div class=\"metric-row\">\n                        <div>\n                            <div style=\"font-weight: 600;\">${area.name}</div>\n                            <div style=\"font-size: 0.8rem; color: #666;\">Priorit√†: ${priority}</div>\n                        </div>\n                        <div style=\"font-weight: bold; color: ${area.score < 55 ? '#dc3545' : area.score < 70 ? '#ffc107' : '#28a745'};\">\n                            ${area.score.toFixed(1)}%\n                        </div>\n                    </div>\n                `;\n            });\n            \n            improvementDiv.innerHTML = html;\n        }\n        \n        function updateDashboardChart() {\n            const ctx = document.getElementById('dashboardChart').getContext('2d');\n            \n            if (charts.dashboard) {\n                charts.dashboard.destroy();\n            }\n            \n            charts.dashboard = new Chart(ctx, {\n                type: 'radar',\n                data: {\n                    labels: [\n                        'Competenze\\nTecniche',\n                        'Performance\\nVendite', \n                        'Soddisfazione\\nClienti',\n                        'Certificazioni', \n                        'Sviluppo\\nBusiness',\n                        'Qualit√†\\nSupporto'\n                    ],\n                    datasets: [{\n                        label: 'Performance Partner',\n                        data: [\n                            scores.technical,\n                            scores.sales,\n                            scores.customer,\n                            scores.certifications,\n                            scores.business,\n                            scores.support\n                        ],\n                        backgroundColor: 'rgba(68, 114, 196, 0.3)',\n                        borderColor: '#4472C4',\n                        borderWidth: 3,\n                        pointBackgroundColor: '#4472C4',\n                        pointRadius: 6\n                    }]\n                },\n                options: {\n                    responsive: true,\n                    maintainAspectRatio: false,\n                    scales: {\n                        r: {\n                            beginAtZero: true,\n                            max: 100,\n                            ticks: {\n                                stepSize: 20,\n                                callback: function(value) {\n                                    return value + '%';\n                                }\n                            },\n                            grid: {\n                                color: '#ddd'\n                            }\n                        }\n                    },\n                    plugins: {\n                        title: {\n                            display: true,\n                            text: 'Partner Performance Overview',\n                            font: {\n                                size: 16,\n                                weight: 'bold'\n                            },\n                            color: '#4472C4'\n                        },\n                        legend: {\n                            display: false\n                        }\n                    }\n                }\n            });\n        }\n        \n        // Initialize on page load\n        document.addEventListener('DOMContentLoaded', function() {\n            // Initialize empty charts\n            updateDashboard();\n        });\n    </script>\n</body>\n</html>"]

